.PHONY : all clean distclean debug install uninstall

#
# Main target and file dependencies:
#

all: $(PROGRAM)

#
# Flags passed to the compiler
#

JX_ROOT = ../Libraries/JX
JX_MAKE_INCLUDE_DIR = $(JX_ROOT)/include/make
include $(JX_MAKE_INCLUDE_DIR)/jx_constants
include $(JX_MAKE_INCLUDE_DIR)/jx_config
CXXDEBUG = -g
CXXOPT = @CXXOPT@
CXXWARN = @CXXWARN@
COPT = @COPT@
CDEBUG = -g
CWARN = @CWARN@
CPPFLAGS = @CPPFLAGS@ $(J_RAW_SYSTEM_STUFF) -IIncludes -I../Sources_Common -IResources -I../Sources_Common/Utilities/ -IHelp -I$(JX_ROOT)/include/jcore -I$(JX_ROOT)/include/jx -I$(JX_ROOT)/include/jximage -I$(JX_ROOT)/ACE/ACE_wrappers $(J_INCLUDE_FREETYPE)
CXXFLAGS = @CXXFLAGS@ -include Sources/Mulberry_Prefix.h $(CPPFLAGS) $(CXXOPT) $(CXXDEBUG) $(CXXWARN)
CFLAGS = @CFLAGS@ -include stdbool.h $(CPPFLAGS) $(COPT) $(CDEBUG) $(CWARN)
LINK_STATIC = @LINK_STATIC@
LINK_DYNAMIC = @LINK_DYNAMIC@
JXLINK = $(JX_LDFLAGS) \
	$(LINK_STATIC)  -ljx-$(JX_LIB_VERSION) -ljcore-$(JX_LIB_VERSION) \
	-lACE-$(ACE_LIB_VERSION) $(LINK_DYNAMIC) \
	$(J_X11_LIBS) @LDAPLIB@ @SSLLIB@ \
	$(LINK_STATIC) $(STL_LIB) $(LINK_DYNAMIC)

LDFLAGS =  $(JXLINK) @LDFLAGS@ -lpthread -ldl
#
# Automatic stuff, should not be changed:
#
DEPENDS = $(OBJS:$O=.dd)

#
# rules:
#
$(PROGRAM): $(OBJS) $(LIBS)
	$(CXX) -o $(PROGRAM) $^ $(LDFLAGS)

Sources/Support/HResourceMap.o: Sources/Support/HResourceMap.cp Resources/xpms.res Resources/xpms.cp

../Plug-ins/CRAM-MD5/sources/md5/md5c.o: ../Plug-ins/CRAM-MD5/sources/md5/md5c.c ../Plug-ins/CRAM-MD5/sources/md5/md5.h
	 $(CC) -c -o $@ $<
install: $(PROGRAM)
	$(INSTALL) -d $(BINDIR)
	$(INSTALL) -m 755 $(PROGRAM) $(BINDIR)
	if [ -n "$(STRIP)" ]; then $(STRIP) $(BINDIR)/$(PROGRAM); else true; fi

uninstall:
	rm -f $(BINDIR)/$(PROGRAM)

clean:
	-rm -rf $(OBJS) $(PROGRAM) *~

distclean:
	-$(MAKE) clean
	-rm -f config.cache config.status config.log Makefile

$(LIBS):
	$(MAKE) $(MAKEFLAGS) -E ../Libraries

#
# Automatic targets and rules:
#
%$O: %.cp
	$(CXX) -c $(CXXFLAGS) $< -o $@


%$O: %.cpp
	$(CXX) -c $(CXXFLAGS) $< -o $@

%$O: %.c
	$(CC) -c $(CFLAGS) $< -o $@

%.dd: %.cp
	@$(SHELL) -ec '$(CXX) -MM $(CXXFLAGS) $< \
	| sed '\''s|\($*\)\.o[ :]*|\1.o $@ : |g'\'' > $@; \
	[ -s $@ ] || rm -f $@'

%.dd: %.cpp
	@$(SHELL) -ec '$(CXX) -MM $(CXXFLAGS) $< \
	| sed '\''s|\($*\)\.o[ :]*|\1.o $@ : |g'\'' > $@; \
	[ -s $@ ] || rm -f $@'

%.dd: %.c
	@$(SHELL) -ec '$(CC) -MM $(CFLAGS) $< \
	| sed '\''s|\($*\)\.o[ :]*|\1.o $@ : |g'\'' > $@; \
	[ -s $@ ] || rm -f $@'

ifeq (yes,${HAS_MM})
-include $(DEPENDS)
endif


